{
  "multipart": [
    {   "apply": { "model": "fd:clear_pane_gray_post" }},
    {   "when": { "north": true },
      "apply": { "model": "fd:clear_pane_gray_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "fd:clear_pane_gray_side", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "fd:clear_pane_gray_side_alt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "fd:clear_pane_gray_side_alt", "y": 90 }
    },
    {   "when": { "north": false },
      "apply": { "model": "fd:clear_pane_gray_noside" }
    },
    {   "when": { "east": false },
      "apply": { "model": "fd:clear_pane_gray_noside_alt" }
    },
    {   "when": { "south": false },
      "apply": { "model": "fd:clear_pane_gray_noside_alt", "y": 90 }
    },
    {   "when": { "west": false },
      "apply": { "model": "fd:clear_pane_gray_noside", "y": 270 }
    }
  ]
}